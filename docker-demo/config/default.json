{
  "server": {
    "port": 9000
  },
  "duckdb": {
    "parquetFile": "data/sydney_buildings.parquet",
    "queryTimeout": 30000,
    "geometryField": "geometry",
    "idField": "id",
    "fields": [
      {
        "name": "id",
        "type": "String",
        "alias": "id"
      },
      {
        "name": "subtype",
        "type": "String",
        "alias": "subtype"
      },
      {
        "name": "sources",
        "type": "String",
        "alias": "sources"
      },
      {
        "name": "names",
        "type": "String",
        "alias": "names"
      }
    ],
    "cacheTtl": 0,
    "crs": 4326,
    "maxRecordCount": 2000,
    "exceededTransferLimit": true,
    "supportsPagination": true
  }
}